@model StudentPresentationSessionModel
@{
	ViewData["Title"] = "ShowQuestionnarieResult";
	Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<link href="~/css/bootstrap.min.css" rel="stylesheet" />
<script src="~/js/bootstrap.min.js"></script>
<script src="~/js/jquery-3.2.1.min.js"></script>
<div class="table-responsive" id="sailorTableArea">
	<table id="sailorTable" class="table table-striped table-bordered" width="100%">

		<thead>
			<tr>
				<th>Öğrenci Numarası</th>
				<th>Öğrenci Sınıfı</th>
				<th>Öğrenci Adı</th>
				<th>Öğrenci Soyadı</th>
				<th>Sunum Öncesi Anketi</th>
				<th>Sunum Sonrası Anketi</th>
			</tr>
		</thead>
		<tbody>
			@foreach (var item in Model.Students)
			{
				<tr>
					<td>@item.StudentSchoolId</td>
					<td>@item.Class.ClassName</td>
					<td>@item.StudentName</td>
					<td>@item.StudentSurname</td>
					<td><button class="btn btn-primary">Görüntüle</button></td>
					<td><button class="btn btn-primary">Görüntüle</button></td>
				</tr>
			}
		</tbody>
	</table>
</div>

